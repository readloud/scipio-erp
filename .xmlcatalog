  <nextCatalog catalog="platform:/resource/scipioce-2.x/framework/entity/dtd/entity-catalog.xml"/>
  <nextCatalog catalog="platform:/resource/scipioce-2.x/framework/minilang/dtd/minilang-catalog.xml"/>
  <nextCatalog catalog="platform:/resource/scipioce-2.x/framework/service/dtd/service-catalog.xml"/>
  <nextCatalog catalog="platform:/resource/scipioce-2.x/framework/testtools/dtd/testtools-catalog.xml"/>
  <nextCatalog catalog="platform:/resource/scipioce-2.x/framework/webapp/dtd/webapp-catalog.xml"/>
