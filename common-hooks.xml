<?xml version="1.0" encoding="UTF-8"?>
<project name="Ant - Common Hooks">
    <dirname property="Ant - Common Hooks.dir" file="${ant.file.Ant - Common Hooks}"/>

    <!-- SCIPIO: This file MUST be imported by all build-hooks.xml files.
            <import file="../../common-hooks.xml" />
        It provides "abstract" dummy hook targets which are overridable.
        It may also provide some common utilities.
        NOTE: build-hooks.xml files are called using inheritall="true", so 
            ${basedir} is actually the root scipio/ofbiz folder.
        DEV NOTE: if you add common utilities here, they must be very uniquely named
            (when in doubt, prefix with scipio-). -->
    
